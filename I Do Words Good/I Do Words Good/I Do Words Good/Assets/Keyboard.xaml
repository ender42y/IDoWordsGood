<?xml version="1.0" encoding="UTF-8"?>
<ContentView xmlns="http://xamarin.com/schemas/2014/forms" 
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="I_Do_Words_Good.Assets.Keyboard">
  <ContentView.Content>
      <StackLayout Orientation="Vertical" VerticalOptions="End">
            <StackLayout Orientation="Horizontal" HorizontalOptions="Center" x:Name="TopRow"/>
            <StackLayout Orientation="Horizontal" HorizontalOptions="Center" x:Name="MiddleRow"/>
            <StackLayout Orientation="Horizontal" HorizontalOptions="Center" x:Name="BottomRow"/>
        </StackLayout>
  </ContentView.Content>
</ContentView>